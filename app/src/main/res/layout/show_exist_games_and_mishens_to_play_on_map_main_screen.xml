<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map1"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".show_exist_games_and_mishens_to_play_on_map_main_screen"
        tools:layout="@layout/map_marker_game_info_window" />

    <RelativeLayout
        android:onClick="buttonShowMishen"
        android:layout_width="match_parent"
        android:layout_height="216dp"
        android:background="@color/black"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="13dp"
            android:layout_marginEnd="312dp"
            android:layout_marginBottom="175dp"
            android:text="@string/maps_show_exist_games_game_subject"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="290dp"
            android:layout_marginBottom="139dp"
            android:text="@string/maps_show_exist_games_game_description"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="314dp"
            android:layout_marginBottom="99dp"
            android:text="@string/maps_show_exist_games_game_kind"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="283dp"
            android:layout_marginBottom="64dp"
            android:text="@string/maps_show_exist_games_mishen_numbers"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textViewSabject"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="128dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="-245dp"
            android:layout_toStartOf="@+id/textView4"
            android:text="TextView"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textViewGameKind"
            android:layout_width="56dp"
            android:layout_height="16dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="119dp"
            android:layout_marginTop="103dp"
            android:layout_marginEnd="-150dp"
            android:layout_marginBottom="97dp"
            android:layout_toStartOf="@+id/textView6"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textViewNumberMishen"
            android:layout_width="61dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="-190dp"
            android:layout_marginBottom="65dp"
            android:layout_toStartOf="@+id/textView7"
            android:textColor="@color/white" />

        <ImageView
            android:id="@+id/buttonShowMishen"
            android:layout_width="47dp"
            android:layout_height="45dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="153dp"
            map:srcCompat="@drawable/tasks" />

        <TextView
            android:id="@+id/textViewTextButten"
            android:layout_width="182dp"
            android:layout_height="41dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="312dp"
            android:layout_marginEnd="13dp"
            android:layout_marginBottom="103dp"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textViewDescription"
            android:layout_width="159dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="133dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="119dp"
            android:layout_marginBottom="137dp"
            android:text="TextView"
            android:textColor="@color/white"
            tools:text="description" />
    </RelativeLayout>

    <Button
        android:id="@+id/buttonConvertToListView"
        android:layout_width="85dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="24dp"
        android:onClick="buttonConvertToListView"
        android:text=" list view"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.116"
        map:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>