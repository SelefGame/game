<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.gms.ads.AdView
        android:id="@+id/adView55"
        android:layout_width="378dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"

        android:layout_marginBottom="4dp"
        map:adSize="LARGE_BANNER"
        map:adUnitId="ca-app-pub-3940256099942544/6300978111"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.515"
        map:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"></com.google.android.gms.ads.AdView>

    <RelativeLayout

        android:id="@+id/relativeLayout2"
        android:layout_width="414dp"
        android:layout_height="367dp"
        android:background="@color/black"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="-2dp">

        <ImageView
            android:id="@+id/buttonGetToThePoint"
            android:layout_width="47dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="270dp"
            android:layout_marginEnd="343dp"
            android:layout_marginBottom="45dp"
            android:onClick="buttonGetToThePoint"
            map:srcCompat="@drawable/map_show" />

        <EditText
            android:id="@+id/editTextclue"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="132dp"
            android:layout_marginEnd="82dp"
            android:layout_marginBottom="185dp"
            android:background="@drawable/text_veiw_border_bleck_for_bacround"
            android:hint=""
            android:textColor="@color/white"
            android:textColorHint="@color/white"
            map:layout_constraintEnd_toEndOf="parent"
            map:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editTextInstraction"
            android:layout_width="wrap_content"
            android:layout_height="49dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="63dp"
            android:layout_marginEnd="82dp"
            android:layout_marginBottom="253dp"
            android:background="@drawable/text_veiw_border_bleck_for_bacround"
            android:hint="@string/Maps_select_update_point_for_Mishen_Main_Instraction_How_To_Go_To_mishen"
            android:textColor="@color/white"
            android:textColorHint="@color/white"
            map:layout_constraintEnd_toEndOf="parent"
            map:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/buttonGetClue"
            android:layout_width="47dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="135dp"
            android:layout_marginEnd="23dp"
            android:layout_marginBottom="180dp"
            android:onClick="buttonGetClue"
            android:hint="רוצה רמז עכשיו"
            map:srcCompat="@drawable/qtrivia" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="307dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="85dp"
            android:layout_marginBottom="317dp"
            android:text="@string/Maps_select_update_point_for_Mishen_Main_Instraction_Info_Title"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textViewTimer"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="23dp"
            android:layout_marginTop="207dp"
            android:layout_marginEnd="83dp"
            android:layout_marginBottom="112dp"
            android:text="xxx"
            android:textColor="@color/white"
            android:textSize="30sp" />

    </RelativeLayout>

    <org.osmdroid.views.MapView
        android:id="@+id/mapView22"
        android:layout_width="414dp"
        android:layout_height="252dp"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        map:layout_constraintBottom_toBottomOf="parent"

        map:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
        map:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>

